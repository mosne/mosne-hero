{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/mosne-hero",
	"version": "0.1.0",
	"title": "Mosne Hero",
	"category": "widgets",
	"icon": "cover-image",
	"description": "A cover block with separate mobile and desktop background images stored in post meta.",
	"example": {},
	"attributes": {
		"desktopImageId": {
			"type": "number",
			"default": 0
		},
		"desktopImageUrl": {
			"type": "string",
			"default": ""
		},
		"mobileImageId": {
			"type": "number",
			"default": 0
		},
		"mobileImageUrl": {
			"type": "string",
			"default": ""
		},
		"overlayColor": {
			"type": "string",
			"default": "#000000"
		},
		"overlayOpacity": {
			"type": "number",
			"default": 0.5
		},
		"minHeight": {
			"type": "number",
			"default": 400
		},
		"contentPosition": {
			"type": "string",
			"default": "center center"
		}
	},
	"supports": {
		"html": false,
		"align": ["wide", "full"],
		"spacing": {
			"padding": true,
			"margin": true
		},
		"__experimentalDefaultControls": {
			"padding": true
		}
	},
	"textdomain": "mosne-hero",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
